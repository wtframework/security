{
  "name": "wtframework/security",
  "description": "What the Framework?! security",
  "license": "proprietary",
  "require": {
    "php": "^8.2",
    "wtframework/config": "^0.1"
  },
  "autoload": {
    "psr-4": {
      "WTFramework\\Security\\": "src/"
